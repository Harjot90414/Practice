<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_15"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".15"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_20"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".20"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_25"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".25"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_30"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".30"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_35"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".35"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_40"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".4"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_50"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".5"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gh_60"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".6"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gh_65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".65"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gh_70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gh_75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gh_80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.8" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gh_85"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_10"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".1"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_15"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".15"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_20"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".2"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_35"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".35"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_40"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".4"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_45"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".45"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_50"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".5" />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_55"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".55"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_60"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".6"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_65"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".65"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_80"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".80"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gh_85"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".85"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/baseline_compare_arrows_24"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_60"
        app:layout_constraintEnd_toEndOf="@id/v_gh_65"
        app:layout_constraintStart_toStartOf="@id/v_gh_35"
        app:layout_constraintTop_toTopOf="@id/h_gh_40" />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="DCA"
        android:gravity="center"
        android:background="@color/blue"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_65"
        app:layout_constraintEnd_toEndOf="@id/v_gh_45"
        app:layout_constraintStart_toStartOf="@id/v_gh_15"
        app:layout_constraintTop_toTopOf="@id/h_gh_35" />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="MARS"
        android:gravity="center"
        android:background="@color/red"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_65"
        app:layout_constraintEnd_toEndOf="@id/v_gh_85"
        app:layout_constraintStart_toStartOf="@id/v_gh_55"
        app:layout_constraintTop_toTopOf="@id/h_gh_35" />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="oneway"
        android:padding="@dimen/dp_10"
        android:background="@color/lightGreen"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_75"
        app:layout_constraintEnd_toEndOf="@id/v_gh_45"
        app:layout_constraintStart_toStartOf="@id/v_gh_10"
        app:layout_constraintTop_toTopOf="@id/h_gh_70" />
    <com.google.android.material.materialswitch.MaterialSwitch
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_75"
        app:layout_constraintEnd_toEndOf="@id/v_gh_45"
        app:layout_constraintTop_toTopOf="@id/h_gh_70"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="itraveller"
        android:gravity="center_vertical"
        android:background="@color/pink"
        android:textColor="@color/white"
        android:padding="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_85"
        app:layout_constraintEnd_toEndOf="@id/v_gh_40"
        app:layout_constraintStart_toStartOf="@id/v_gh_10"
        app:layout_constraintTop_toTopOf="@id/h_gh_80" />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="fight"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_30"
        app:layout_constraintEnd_toEndOf="@id/v_gh_60"
        app:layout_constraintStart_toStartOf="@id/v_gh_40"
        app:layout_constraintTop_toTopOf="@id/h_gh_25"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/baseline_rocket_24"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_25"
        app:layout_constraintEnd_toEndOf="@id/v_gh_60"
        app:layout_constraintStart_toStartOf="@id/v_gh_40"
        app:layout_constraintTop_toTopOf="@id/h_gh_15"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Rovers"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_30"
        app:layout_constraintEnd_toEndOf="@id/v_gh_85"
        app:layout_constraintStart_toStartOf="@id/v_gh_65"
        app:layout_constraintTop_toTopOf="@id/h_gh_25"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/baseline_satellite_alt_24"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_25"
        app:layout_constraintEnd_toEndOf="@id/v_gh_85"
        app:layout_constraintStart_toStartOf="@id/v_gh_65"
        app:layout_constraintTop_toTopOf="@id/h_gh_15"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="space"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_30"
        app:layout_constraintEnd_toEndOf="@id/v_gh_35"
        app:layout_constraintStart_toStartOf="@id/v_gh_15"
        app:layout_constraintTop_toTopOf="@id/h_gh_25"
        />
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/baseline_satellite_alt_24"
        app:layout_constraintBottom_toBottomOf="@id/h_gh_25"
        app:layout_constraintEnd_toEndOf="@id/v_gh_35"
        app:layout_constraintStart_toStartOf="@id/v_gh_15"
        app:layout_constraintTop_toTopOf="@id/h_gh_15"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
